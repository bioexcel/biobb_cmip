{
    "_id": "biobb_cmip",
    "desc": "Biobb_cmip is the Biobb module collection to compute classical molecular interaction potentials.",
    "github": "https://github.com/bioexcel/biobb_cmip",
    "readthedocs": "https://biobb-cmip.readthedocs.io/en/latest/",
    "conda": "https://anaconda.org/bioconda/biobb_cmip",
    "docker": "https://quay.io/biocontainers/biobb_cmip:3.6.0--pyhdfd78af_0",
    "singularity": "https://www.singularity-hub.org/collections/",
    "version": "3.6.0",
    "tools" : [
        {
            "block" : "Cmip",
            "tool" : "CMIP cmip",
            "desc" : "CMIP cmip module compute classical molecular interaction potentials.",
            "exec" : "cmip",
            "docs": "https://biobb-cmip.readthedocs.io/en/latest/cmip.html#module-cmip.cmip"
        },
        {
            "block" : "Titration",
            "tool" : "CMIP titration",
            "desc" : "Creates a GROMACS structure file (GRO) adding the information of the solvent box to the input structure file.",
            "exec" : "titration",
            "docs": "https://biobb-cmip.readthedocs.io/en/latest/cmip.html#module-titration.titration"
        }
    ],
    "dep_pypi" : [
        "install_requires=['biobb_common==3.6.0']",
        "python_requires='==3.7.*'"
    ],
    "dep_conda" : [
        "python ==3.7.*",
        "biobb_common ==3.6.0",
        "cmip ==2.7.0"
    ],
    "keywords" : [
        "classical",
        "molecular",
        "interaction",
        "potentials"
    ]
}
