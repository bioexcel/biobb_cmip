{
    "_id": "biobb_cmip",
    "desc": "Biobb_cmip is the Biobb module collection to compute classical molecular interaction potentials.",
    "github": "https://github.com/bioexcel/biobb_cmip",
    "readthedocs": "https://biobb-cmip.readthedocs.io/en/latest/",
    "conda": "https://anaconda.org/bioconda/biobb_cmip",
    "docker": "https://quay.io/biocontainers/biobb_cmip:3.7.8--pyhdfd78af_0",
    "singularity": "https://depot.galaxyproject.org/singularity/biobb_cmip:3.7.8--pyhdfd78af_0",
    "version": "3.9.0",
    "tools" : [
        {
            "block" : "Cmip",
            "tool" : "CMIP cmip",
            "desc" : "CMIP cmip module compute classical molecular interaction potentials.",
            "exec" : "cmip",
            "docs": "https://biobb-cmip.readthedocs.io/en/latest/cmip.html#module-cmip.cmip"
        },
        {
            "block" : "Titration",
            "tool" : "CMIP titration",
            "desc" : "CMIP cmip module compute classical molecular interaction potentials.",
            "exec" : "titration",
            "docs": "https://biobb-cmip.readthedocs.io/en/latest/cmip.html#module-cmip.titration"
        },
        {
            "block" : "PrepareStructure",
            "tool" : "Prepare Structure",
            "desc" : "Creates a CMIP suitable PDB from an standar structure file",
            "exec" : "prepare_structure",
            "docs": "https://biobb-cmip.readthedocs.io/en/latest/cmip.html#module-cmip.prepare_structure"
        },
        {
            "block" : "PreparePDB",
            "tool" : "Prepare PDB",
            "desc" : "Creates a CMIP suitable PDB from an standar PDB file",
            "exec" : "prepare_pdb",
            "docs": "https://biobb-cmip.readthedocs.io/en/latest/cmip.html#module-cmip.prepare_pdb"
        },
        {
            "block" : "IgnoreResidues",
            "tool" : "Ignore Residues",
            "desc" : "Mark residues which charges will be ignored in the CMIP potential calculations.",
            "exec" : "ignore_residues",
            "docs": "https://biobb-cmip.readthedocs.io/en/latest/cmip.html#module-cmip.ignore_residues"
        }
    ],
    "dep_pypi" : [
        "install_requires=['biobb_common==3.9.0', 'mdanalysis==2.0.0', 'biobb_structure_checking==3.12.1']",
        "python_requires='>=3.7'"
    ],
    "dep_conda" : [
        "python >=3.7",
        "biobb_common ==3.9.0",
        "cmip ==2.7.0",
        "mdanalysis==2.0.0",
        "biobb_structure_checking==3.12.1"
    ],
    "keywords" : [
        "classical",
        "molecular",
        "interaction",
        "potentials"
    ]
}
